// program for print array in reverse /
#include <stdio.h>
int main() {
    int a[5];
    int i;
    printf("enter the value of array");
    for(i=0;i<=4;i++){
        scanf("%d",&a[i]);
    }
  printf("\n value of array in reverse ");
  for(i=4;i<=0;i--)
  {
      scanf("%d",&a[i]);
  }
    return 0;
}